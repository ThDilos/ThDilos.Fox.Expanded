	"transform_or_show_karma": {
		"type": "origins:active_self",
		"key": {
			"key": "key.loadToolbarActivator"
		},
		"entity_action": {
			"type": "origins:if_else_list",
			"actions": [
				{
					"condition": {
						"type": "origins:and",
						"conditions": [
							{
								"type": "origins:resource",
								"resource": "*:thkarma_unlockable",
								"compare_to": 1,
								"comparison": "=="
							},
							{
								"type": "origins:sneaking"
							}
						]
					},
					"action": {
						"type": "origins:change_resource",
						"resource": "*:thkarma_show_karma",
						"change": 60,
						"operation": "set"
					}
				},
				{
					"condition": {
						"type": "origins:resource",
						"resource": "*:*_transformable",
						"compare_to": 0,
						"comparison": "=="
					},
					"action": {
						"type": "origins:execute_command",
						"command": "/title @s actionbar {\"translate\":\"origins.thorigins.untransformable\",\"color\":\"red\",\"bold\":\"true\"}"
					}
				},
				{
					"condition": {
						"type": "origins:and",
						"conditions": [
							{
								"type": "origins:resource",
								"resource": "*:*_cooldown",
								"compare_to": 0,
								"comparison": "=="
							},
							{
								"type": "origins:or",
								"conditions": [
									{
										"type": "origins:resource",
										"resource": "*:*_lock_lightning",
										"compare_to": 0,
										"comparison": "=="
									},
									{
										"type": "origins:resource",
										"resource": "*:*_lock_fire",
										"compare_to": 0,
										"comparison": "=="
									},
									{
										"type": "origins:resource",
										"resource": "*:*_lock_kitsune",
										"compare_to": 0,
										"comparison": "=="
									},
									{
										"type": "origins:resource",
										"resource": "*:*_lock_ninetails",
										"compare_to": 0,
										"comparison": "=="
									}
								]
							},
							{
								"type": "origins:or",
								"conditions": [
									{
										"type": "origins:origin",
										"origin": "thorigins:fox",
										"layer": "origins:origin"
									},
									{
										"type": "origins:origin",
										"origin": "thorigins:element_fox",
										"layer": "origins:origin"
									}
								]
							}
						]
					},
					"action": {
						"type": "origins:if_else",

						"condition": {
							"type": "origins:resource",
							"resource": "*:fox/thfoxify_afk_timer",
							"compare_to": 0,
							"comparison": "=="
						},
						"if_action": {
							"type": "origins:execute_command",
							"command": "/title @s actionbar {\"translate\":\"origins.thorigins.sleepin_notransform\",\"color\":\"red\",\"bold\":\"true\"}"
						},
						"else_action": {
							"type": "origins:and",
							"actions": [
								{
									"type": "origins:change_resource",
									"resource": "*:*_temp_disable_sleepin",
									"change": 1,
									"operation": "set"
								},
								{
									"type": "origins:execute_command",
									"command": "/origin set @s origins:origin thorigins:element_fox"
								},
								{
									"type": "origins:execute_command",
									"command": "/origin gui @s thorigins:origin"
								},
								{
									"type": "origins:execute_command",
									"command": "/scale reset pehkui:reach @s"
								},
								{
									"type": "origins:change_resource",
									"resource": "*:*_cooldown",
									"change": 600,
									"operation": "set"
								}
							]
						}
					}
				},
				{
					"condition": {
						"type": "origins:constant",
						"value": true
					},
					"action": {
						"type": "origins:if_else",
						"condition": {
							"type": "origins:resource",
							"resource": "*:*_cooldown",
							"compare_to": 0,
							"comparison": "=="
						},
						"if_action": {
							"type": "origins:execute_command",
							"command": "/title @s actionbar {\"translate\":\"origins.thorigins.no_element\",\"color\":\"red\",\"bold\":\"true\"}"
						}
					}
				}
			]
		}
	},

