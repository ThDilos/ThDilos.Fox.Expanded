{
	"type": "origins:multiple",
	"hidden": true,

	"_init_": {
		"type": "origins:action_on_callback",
		"entity_action_gained": {
			"type": "origins:and",
			"actions": [
				{
					"type": "origins:execute_command",
					"command": "/data merge entity @s {Silent:1b,Invulnerable:1b,Invisible:1b,NoBasePlate:1b,DisabledSlots:4144959,CustomName:'[{\"text\":\"Ice Fox Frost Walker\"}]',Invulnerable:1b,NoGravity:1b,PersistenceRequired:1b,Silent:1b,Tags:[\"icefoxpaws\"],ArmorItems:[{id:leather_boots,tag:{Enchantments:[{id:frost_walker,lvl:2}],Unbreakable:1b},Count:1}],ArmorDropChances:[0f]}"
				},
				{
					"type": "origins:execute_command",
					"command": "/scale set pehkui:hitbox_height 0.01 @s"
				},
				{
					"type": "origins:execute_command",
					"command": "/scale set pehkui:hitbox_width 0.01 @s"
				},
				{
					"type": "origins:execute_command",
					"command": "/scale set pehkui:model_height 0.01 @s"
				},
				{
					"type": "origins:execute_command",
					"command": "/scale set pehkui:model_width 0.01 @s"
				}
			]
		}
	},

	"invis": {
		"type": "origins:invisibility",
		"render_armor": false,
		"render_outline": false
	},

	"invis_high_version": {
		"type": "origins:prevent_feature_render",
		"features": ["elytra", "cape", "held_item", "head"],
		"condition": {
			"type": "origins:power_active",
			"power": "*:*_invis"
		}
	},

	"ticks": {
		"type": "origins:action_over_time",
		"interval": 1,
		"entity_action": {
			"type": "origins:execute_command",
			"command": "/kill @s"
		},
		"condition": {
			"type": "origins:fluid_height",
			"fluid": "minecraft:lava",
			"compare_to": 0,
			"comparison": ">"
		}
	},

	"ticks_20": {
		"type": "origins:action_over_time",
		"interval": 20,
		"entity_action": {
			"type": "origins:and",
			"actions": [
				{
					"type": "origins:if_else",
					"condition": {
						"type": "origins:command",
						"command": "/execute if entity @e[type=boat,distance=..8]",
						"compare_to": 0,
						"comparison": ">"
					},
					"if_action": {
						"type": "origins:execute_command",
						"command": "/item replace entity @s armor.feet with minecraft:leather_boots[minecraft:enchantments={frost_walker:4}]"
					},
					"else_action": {
						"type": "origins:execute_command",
						"command": "/item replace entity @s armor.feet with minecraft:leather_boots[minecraft:enchantments={frost_walker:1}]"
					}
				},
				{
					"type": "origins:if_else",
					"condition": {
						"type": "origins:command",
						"command": "/execute if entity @a[tag=foxie,distance=..8]",
						"compare_to": 0,
						"comparison": "=="
					},
					"if_action": {
						"type": "origins:execute_command",
						"command": "/kill @s"
					}
				}
			]
		}
	}
}
