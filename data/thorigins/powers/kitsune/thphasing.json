{
	"type": "origins:multiple",
	"badges": [
		{
			"type": "origins:keybind",
			"sprite": "origins:textures/gui/badge/toggle.png",
			"text": "origins.gui.badge.active"
		}
	],

	"initiator": {
		"type": "origins:active_self",
		"key": "key.origins.primary_active",
		"entity_action": {
			"type": "origins:toggle",
			"power": "*:*_toggle"
		},
		"condition": {
			"type": "origins:and",
			"conditions": [
				{
					"type": "origins:resource",
					"resource": "*:kitsune/thchoice_choice",
					"compare_to": 1,
					"comparison": "=="
				},
				{
					"type": "origins:resource",
					"resource": "*:thtransform_souls",
					"compare_to": 0,
					"comparison": ">"
				}
			]
		}
	},

	"toggle": {
		"type": "origins:toggle",
		"active_by_default": false,
		"key": "nuh.uh"
	},

	"phasing": {
		"type": "origins:phasing",
		"condition": {
			"type": "origins:power_active",
			"power": "*:*_toggle"
		},
		"view_distance": 20
	},

	"immunity_inwall": {
		"type": "origins:invulnerability",
		"damage_condition": {
			"type": "origins:name",
			"name": "inWall"
		},
		"condition": {
			"type": "origins:power_active",
			"power": "*:*_toggle"
		}
	},

	"screen_overlay": {
		"type": "origins:overlay",
		"red": 0.1,
		"green": 0.1,
		"blue": 0.1,
		"texture": "minecraft:textures/misc/nausea.png",
		"draw_mode": "nausea",
		"draw_phase": "below_hud",
		"condition": {
			"type": "origins:power_active",
			"power": "*:*_toggle"
		}
	},

	"ticks": {
		"type": "origins:action_over_time",
		"interval": 1,
		"entity_action": {
			"type": "origins:and",
			"actions": [
				{
					"type": "origins:if_else",
					"condition": {
						"type": "origins:power_active",
						"power": "*:*_toggle"
					},
					"if_action": {
						"type": "origins:and",
						"actions": [
							{
								"type": "origins:change_resource",
								"resource": "*:thtransform_souls",
								"change": -1,
								"operation": "add"
							},
							{
								"type": "origins:change_resource",
								"resource": "*:kitsune/thheal_soul_regen_delay",
								"change": 100,
								"operation": "set"
							},
							{
								"type": "origins:set_fall_distance",
								"fall_distance": 0
							}
						]
					}
				},
				{
					"type": "origins:if_else",
					"condition": {
						"type": "origins:and",
						"conditions": [
							{
								"type": "origins:power_active",
								"power": "*:*_toggle"
							},
							{
								"type": "origins:resource",
								"resource": "*:kitsune/thchoice_choice",
								"compare_to": 1,
								"comparison": "!="
							}
						]
					},
					"if_action": {
						"type": "origins:toggle",
						"power": "*:*_toggle"
					}
				},
				{
					"type": "origins:if_else",
					"condition": {
						"type": "origins:and",
						"conditions": [
							{
								"type": "origins:power_active",
								"power": "*:*_toggle"
							},
							{
								"type": "origins:resource",
								"resource": "*:thtransform_souls",
								"compare_to": 0,
								"comparison": "=="
							}
						]
					},
					"if_action": {
						"type": "origins:toggle",
						"power": "*:*_toggle"
					}
				}
			]
		}
	},

	"transparentify": {
		"type": "origins:model_color",
		"alpha": 0.5,
		"condition": {
			"type": "origins:power_active",
			"power": "*:*_toggle"
		}
	},

	"float_initiator": {
		"type": "origins:active_self",
		"key": {
			"key": "key.jump",
			"continuous": true
		},
		"entity_action": {
			"type": "origins:change_resource",
			"resource": "*:*_float_detector",
			"change": 2,
			"operation": "set"
		},
		"condition": {
			"type": "origins:power_active",
			"power": "*:*_toggle"
		}
	},

	"float_detector": {
		"type": "origins:resource",
		"min": 0,
		"max": 2,
		"hud_render": {
			"should_render": false
		}
	},

	"auto_unfloat": {
		"type": "origins:action_over_time",
		"interval": 1,
		"entity_action": {
			"type": "origins:change_resource",
			"resource": "*:*_float_detector",
			"change": -1,
			"operation": "add"
		}
	},

	"float_on_air": {
		"type": "origins:climbing",
		"condition": {
			"type": "origins:resource",
			"resource": "*:*_float_detector",
			"compare_to": 0,
			"comparison": ">"
		}
	},

	"walk_on_air": {
		"type": "origins:modify_velocity",
		"axes": ["y"],
		"modifier": {
			"operation": "set_total",
			"value": 0
		},
		"condition": {
			"type": "origins:and",
			"conditions": [
				{
					"type": "origins:power_active",
					"power": "*:*_toggle"
				},
				{
					"type": "origins:resource",
					"resource": "*:*_float_detector",
					"compare_to": 0,
					"comparison": "=="
				},
				{
					"type": "origins:sneaking",
					"inverted": true
				}
			]
		}
	},

	"modify_down_speed": {
		"type": "origins:modify_falling",
		"velocity": 0.02,
		"take_fall_damage": false,
		"condition": {
			"type": "origins:power_active",
			"power": "*:*_toggle"
		}
	},

	"modify_total_motion": {
		"type": "origins:action_over_time",
		"interval": 1,
		"entity_action": {
			"type": "origins:if_else",
			"condition": {
				"type": "origins:power_active",
				"power": "*:*_toggle"
			},
			"if_action": {
				"type": "origins:execute_command",
				"command": "/scale set pehkui:motion 2 @s"
			},
			"else_action": {
				"type": "origins:execute_command",
				"command": "/scale reset pehkui:motion @s"
			}
		}
	},

	"unable_to_melee": {
		"type": "origins:action_over_time",
		"interval": 1,
		"condition": {
			"type": "origins:power_active",
			"power": "*:*_toggle"
		},
		"entity_action": {
			"type": "origins:apply_effect",
			"effect": {
				"effect": "weakness",
				"amplifier": 99,
				"duration": 5,
				"is_ambient": true,
				"show_icon": false,
				"show_particles": false
			}
		}
	},

	"unable_to_shoot": {
		"type": "origins:prevent_item_use",
		"item_condition": {
			"type": "origins:ingredient",
			"ingredient": [
				{
					"item": "bow"
				},
				{
					"item": "crossbow"
				},
				{
					"item": "trident"
				}
			]
		},
		"condition": {
			"type": "origins:power_active",
			"power": "*:*_toggle"
		}
	},

	"damage_reduction": {
		"type": "origins:modify_damage_taken",
		"modifier": {
			"value": -0.5,
			"operation": "multiply_total"
		},
		"condition": {
			"type": "origins:power_active",
			"power": "*:*_toggle"
		}
	}
}
