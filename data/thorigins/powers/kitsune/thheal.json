					}
				},
				{
					"type": "origins:actor_action",
					"action": {
						"type": "origins:and",
						"actions": [
							{
								"type": "origins:change_resource",
								"resource": "*:*_energy_regen_delay",
								"change": 100,
								"operation": "set"
							},
							{
								"type": "origins:change_resource",
								"resource": "*:*_energy",
								"change": -50
							},
							{
								"type": "origins:heal",
								"amount": 2
							}
						]
					}
				}
			]
		},
		"bientity_condition": {
			"type": "origins:and",
			"conditions": [
				{
					"type": "origins:actor_condition",
					"condition": {
						"type": "origins:and",
						"conditions": [
							{
								"type": "origins:resource",
								"resource": "*:kitsune/thchoice_choice",
								"compare_to": 3,
								"comparison": "=="
							},
							{
								"type": "origins:equipped_item",
								"equipment_slot": "mainhand",
								"item_condition": {
									"type": "origins:empty"
								}
							}
						]
					}
				},
				{
					"type": "origins:target_condition",
					"condition": {
						"type": "origins:relative_health",
						"compare_to": 1,
						"comparison": "<"
					}
				}
			]
		}
	}
