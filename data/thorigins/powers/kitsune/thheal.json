				{
					"translate": "origin.thorigins.general"
				},
				{
					"keybind": "key.origins.primary_active"
				},
				{
					"translate": "origin.thorigins.thheal1"
				}
			]
		},
		{
			"type": "origins:tooltip",
			"sprite": "origins:textures/gui/badge/active.png",
			"text": [
				{
					"translate": "origin.thorigins.general"
				},
				{
					"keybind": "key.origins.primary_active"
				},
				{
					"translate": "origin.thorigins.thheal3"
				}
			]
		},
		{
			"type": "origins:tooltip",
			"sprite": "origins:textures/gui/badge/active.png",
			"text": [
				{
					"translate": "origin.thorigins.general"
				},
				{
					"keybind": "key.use"
				},
				{
					"translate": "origin.thorigins.thheal2"
				}
			]
		}
	],

	"energy": {
		"type": "origins:resource",
		"min": 0,
		"max": 1000,
		"start_value": 1000,
		"hud_render": {
			"should_render": true,
			"bar_index": 14,
			"sprite_location": "origins:textures/gui/community/huang/resource_bar_01.png",
			"condition": {
				"type": "origins:resource",
				"resource": "*:*_energy",
				"compare_to": 1000,
				"comparison": "<"
			}
		}
	},

	"energy_regen_delay": {
		"type": "origins:resource",
		"min": 0,
		"max": 100,
		"start_value": 0,
		"hud_render": {
			"should_render": false
		}
	},

	"energy_regen": {
		"type": "origins:action_over_time",
		"interval": 1,
		"entity_action": {
			"type": "origins:if_else",
			"condition": {
				"type": "origins:resource",
				"resource": "*:*_energy_regen_delay",
				"compare_to": 0,
				"comparison": "=="
			},
			"if_action": {
				"type": "origins:change_resource",
				"resource": "*:*_energy",
				"change": 5,
				"operation": "add"
			},
			"else_action": {
				"type": "origins:change_resource",
				"resource": "*:*_energy_regen_delay",
				"change": -1,
				"operation": "add"
			}
		}
	},

	"aura_glow": {
		"type": "origins:self_glow",
		"green": 1.0,
		"red": 0.5,
		"blue": 0.5,
		"condition": {
			"type": "origins:power_active",
			"power": "*:*_healing_aura_switch"
		}
	},

	"healing_aura_switch": {
		"type": "origins:toggle",
		"key": "nuh.uh",
		"active_by_default": false,
		"retain_state": true
	},

	"aura_initiator": {
		"type": "origins:active_self",
		"key": {
			"key": "key.origins.primary_active"
		},
		"cooldown": 0,
		"entity_action": {
			"type": "origins:if_else",
			"condition": {
				"type": "origins:and",
				"conditions": [
					{
						"type": "origins:resource",
						"resource": "*:kitsune/thchoice_choice",
						"compare_to": 3,
						"comparison": "=="
					},
					{
						"type": "origins:resource",
						"resource": "*:*_energy",
						"compare_to": 0,
						"comparison": ">"
					}
				]
			},
			"if_action": {
				"type": "origins:if_else",
				"condition": {
					"type": "origins:sneaking"
				},
				"else_action": {
					"type": "origins:toggle",
					"power": "*:*_healing_aura_switch"
				},
				"if_action": {
					"type": "origins:and",
					"actions": [
						{
							"type": "origins:change_resource",
							"resource": "*:*_energy",
							"change": -100
						},
						{
							"type": "origins:change_resource",
							"resource": "*:*_energy_regen_delay",
							"change": 100,
							"operation": "set"
						},
						{
							"type": "origins:spawn_particles",
							"particle": "minecraft:wax_on",
							"count": 10,
							"force": false,
							"offset_y": 0.5,
							"spread": {
								"x": 10,
								"y": 1,
								"z": 10
							},
							"speed": 0
						},
						{
							"type": "origins:execute_command",
							"command": "/playsound minecraft:block.beacon.activate player @s ~ ~ ~ 0.8 2"
						},
						{
							"type": "origins:execute_command",
							"command": "/effect give @e[distance=0.1..10] minecraft:strength 30 0 true"
						},
						{
							"type": "origins:execute_command",
							"command": "/effect give @e[distance=0.1..10] minecraft:resistance 30 0 true"
						},
						{
							"type": "origins:execute_command",
							"command": "/effect give @e[distance=0.1..10] minecraft:speed 30 0 true"
						},
						{
							"type": "origins:execute_command",
							"command": "/effect give @e[distance=0.1..10] minecraft:luck 30 0 true"
						}
					]
				}
			}
		}
	},

	"healing_aura": {
		"type": "origins:action_over_time",
		"interval": 10,
		"entity_action": {
			"type": "origins:and",
			"actions": [
				{
					"type": "origins:if_else",
					"condition": {
						"type": "origins:power_active",
						"power": "*:*_healing_aura_switch"
					},
					"if_action": {
						"type": "origins:and",
						"actions": [
							{
								"type": "origins:change_resource",
								"resource": "*:*_energy_regen_delay",
								"change": 100,
								"operation": "set"
							},
							{
								"type": "origins:execute_command",
								"command": "/playsound minecraft:block.beacon.ambient player @s ~ ~ ~ 0.5 1.5"
							},
							{
								"type": "origins:spawn_particles",
								"particle": "minecraft:glow",
								"count": 10,
								"force": false,
								"offset_y": 0.5,
								"spread": {
									"x": 10,
									"y": 1,
									"z": 10
								},
								"speed": 0
							},
							{
								"type": "origins:spawn_particles",
								"particle": "minecraft:wax_off",
								"count": 10,
								"force": false,
								"offset_y": 0.5,
								"spread": {
									"x": 10,
									"y": 1,
									"z": 10
								},
								"speed": 0
							},
							{
								"type": "origins:change_resource",
								"resource": "*:*_energy",
								"change": -10
							},
							{
								"type": "origins:execute_command",
								"command": "/effect give @e[distance=0.1..10] minecraft:regeneration 1 0 true"
							}
						]
					}
				},
				{
					"type": "origins:if_else",
					"condition": {
						"type": "origins:and",
						"conditions": [
							{
								"type": "origins:or",
								"conditions": [
									{
										"type": "origins:resource",
										"resource": "*:*_energy",
										"compare_to": 0,
										"comparison": "=="
									},
									{
										"type": "origins:resource",
										"resource": "*:kitsune/thchoice_choice",
										"compare_to": 3,
										"comparison": "!="
									}
								]
							},
							{
								"type": "origins:power_active",
								"power": "*:*_healing_aura_switch"
							}
						]
					},
					"if_action": {
						"type": "origins:toggle",
						"power": "*:*_healing_aura_switch"
					}
				}
			]
		}
	},

	"boop_heal": {
		"type": "origins:action_on_entity_use",
		"bientity_action": {
			"type": "origins:and",
			"actions": [
