{
	"type": "origins:multiple",
	"name": "ninetails.thphasing.name",
	"description": "ninetails.thphasing.description",
	"badges": [
		{
			"type": "origins:tooltip",
			"sprite": "origins:textures/gui/badge/toggle.png",
			"text": [
				{
					"translate": "thorigins.press0"
				},
				{
					"keybind": "key.origins.primary_active",
					"color": "aqua"
				},
				{
					"translate": "ninetails.thphasing.key"
				}
			]
		},
		{
			"type": "origins:keybind",
			"sprite": "origins:textures/gui/badge/info.png",
			"text": "ninetails.thphasing.info"
		}
	],

	"ticks": {
		"type": "origins:action_over_time",
		"interval": 4,
		"entity_action": {
			"type": "origins:if_else",
			"condition": {
				"type": "origins:power_active",
				"power": "*:*_toggle"
			},
			"if_action": {
				"type": "origins:and",
				"actions": [
					{
						"type": "origins:change_resource",
						"resource": "*:thtransform_souls",
						"change": -2
					},
					{
						"type": "origins:set_fall_distance",
						"fall_distance": 0
					},
					{
						"type": "origins:execute_command",
						"command": "/scale set pehkui:motion 3 @s"
					},
					{
						"type": "origins:apply_effect",
						"effect": {
							"effect": "weakness",
							"amplifier": 99,
							"duration": 5,
							"is_ambient": true,
							"show_icon": false,
							"show_particles": false
						}
					}
				]
			},
			"else_action": {
				"type": "origins:execute_command",
				"command": "/scale reset pehkui:motion @s"
			}
		}
	},

	"toggle": {
		"type": "origins:toggle",
		"active_by_default": false,
		"retain_state": false,
		"key": "key.origins.primary_active",
		"condition": {
			"type": "origins:and",
			"conditions": [
				{
					"type": "origins:resource",
					"resource": "*:ninetails/thchoice_choice",
					"compare_to": 1,
					"comparison": "=="
				},
				{
					"type": "origins:resource",
					"resource": "*:thtransform_souls",
					"compare_to": 0,
					"comparison": ">"
				},
				{
					"type": "origins:resource",
					"resource": "*:fox/thfoxify_afk_timer",
					"compare_to": 1,
					"comparison": ">"
				}
			]
		}
	},

	"phasing": {
		"type": "origins:phasing",
		"condition": {
			"type": "origins:power_active",
			"power": "*:*_toggle"
		},
		"view_distance": 20
	},

	"immunity_inwall": {
		"type": "origins:invulnerability",
		"damage_condition": {
			"type": "origins:name",
			"name": "inWall"
		},
		"condition": {
			"type": "origins:power_active",
			"power": "*:*_toggle"
		}
	},

	"screen_overlay": {
		"type": "origins:overlay",
		"red": 0.1,
		"green": 0.1,
		"blue": 0.1,
		"texture": "minecraft:textures/misc/nausea.png",
		"draw_mode": "nausea",
		"draw_phase": "below_hud",
		"condition": {
			"type": "origins:power_active",
			"power": "*:*_toggle"
		}
	},

	"transparentify": {
		"type": "origins:model_color",
		"alpha": 0.5,
		"condition": {
			"type": "origins:power_active",
			"power": "*:*_toggle"
		}
	},

	"float_initiator": {
		"type": "origins:active_self",
		"key": {
			"key": "key.jump",
			"continuous": true
		},
		"entity_action": {
			"type": "origins:trigger_cooldown",
			"power": "*:*_float_detector"
		},
		"condition": {
			"type": "origins:power_active",
			"power": "*:*_toggle"
		}
	},

	"float_detector": {
		"type": "origins:cooldown",
		"cooldown": 2,
		"hud_render": { "should_render": false }
	},

	"float_on_air": {
		"type": "origins:climbing",
		"condition": {
			"type": "origins:resource",
			"resource": "*:*_float_detector",
			"compare_to": 0,
			"comparison": ">"
		}
	},

	"walk_on_air": {
		"type": "origins:modify_velocity",
		"axes": ["y"],
		"modifier": {
			"operation": "set_total",
			"value": 0
		},
		"condition": {
			"type": "origins:and",
			"conditions": [
				{
					"type": "origins:power_active",
					"power": "*:*_toggle"
				},
				{
					"type": "origins:resource",
					"resource": "*:*_float_detector",
					"compare_to": 0,
					"comparison": "=="
				},
				{
					"type": "origins:sneaking",
					"inverted": true
				}
			]
		}
	},

	"modify_down_speed": {
		"type": "origins:modify_falling",
		"velocity": 0.02,
		"take_fall_damage": false,
		"condition": {
			"type": "origins:power_active",
			"power": "*:*_toggle"
		}
	},

	"unable_to_shoot": {
		"type": "origins:prevent_item_use",
		"item_condition": {
			"type": "origins:ingredient",
			"ingredient": [
				{
					"item": "bow"
				},
				{
					"item": "crossbow"
				},
				{
					"item": "trident"
				}
			]
		},
		"condition": {
			"type": "origins:power_active",
			"power": "*:*_toggle"
		}
	},

	"damage_reduction": {
		"type": "origins:modify_damage_taken",
		"modifier": {
			"value": -0.25,
			"operation": "multiply_total"
		},
		"condition": {
			"type": "origins:power_active",
			"power": "*:*_toggle"
		}
	}
}
